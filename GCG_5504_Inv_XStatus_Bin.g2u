Program.Sub.LoadDtXStatus.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.sSQL.Declare
	V.Local.dCutoffDate.Declare
	V.Local.sPart.Declare
	V.Local.sActiveFilterString.Declare

	Gui.frmXStatus.lblStatus.Visible(True)
	Gui.frmXStatus.pbStatus.Visible(True)
	Gui.frmXStatus.pbStatus.Min(0)
	Gui.frmXStatus.pbStatus.Max(100)

	F.ODBC.Connection!con.OpenCompanyConnection(300)

	F.Intrinsic.Control.If(V.Global.bLoadDate)
		F.Intrinsic.Control.CallSub(Serialize)
	F.Intrinsic.Control.EndIf
	V.Global.bLoadDate.Set(True)

	F.Intrinsic.Control.If(V.DataTable.dtXStatus.Exists)
		F.Data.DataTable.Close("dtXStatus")
	F.Intrinsic.Control.EndIf

'Function.Intrinsic.Debug.InvokeDebugger
'	Function.Intrinsic.Debug.Stop


	F.Intrinsic.Control.If(V.Screen.frmXStatus!chkCutoff.Value,=,0)
		F.Data.DataTable.CreateFromSQL("dtXStatus","con","Select rtrim(A.Part) as Part,RTrim(A.Description) As Description, RTrim(RTrim(B.Description_2)+' '+RTrim(B.Description_3)) as AltDesc, RTrim(D.Text) As ExtraDesc,A.Product_Line,Case B.Code_Source When 'P' Then 'PS' When 'J' Then 'PJ' When '' Then 'PJ' When 'M' Then 'MS' When 'F' Then 'MJ' Else B.Code_Source End As Code_Source,A.UM_Inventory,A.Time_Material_Lead,A.Code_Sort,RTrim(A.Bin) As Bin, A.Width, A.Code_ABC,Cast(Null As Date) As Date_History,A.Qty_OnHand,A.Qty_Reorder,A.Qty_OnOrder_PO+A.Qty_OnOrder_WO As OnOrder,A.Qty_Required,A.Qty_Usage_Mo_01+A.Qty_Usage_Mo_02+A.Qty_Usage_Mo_03+A.Qty_Usage_Mo_04+A.Qty_Usage_Mo_05+A.Qty_Usage_Mo_06+A.Qty_Usage_Mo_07+A.Qty_Usage_Mo_08+A.Qty_Usage_Mo_09+A.Qty_Usage_Mo_10+A.Qty_Usage_Mo_11+A.Qty_Usage_Mo_12 As Usage_Last_12_Mos,A.Amt_Price, C.Cost, C.Matl, C.Labor, C.Ovhd, C.Outs, C.Freight, C.Oth,A.Qty_OnHand*A.Amt_Price As Ext_Price,Cast(Case When Flag_Lot = '' Or Flag_Lot = 'N' Then 0 Else 1 End As Bit) As Flag_Lot,Cast(Case When Flag_Drop = '' Or Flag_Drop = 'N' Then 0 Else 1 End As Bit) As Flag_Drop,Cast(Case When Flag_Serialize = '' Or Flag_Serialize = 'N' Then 0 Else 1 End As Bit) As Flag_Serialize,Cast(Case When Obsolete_Flag = '' Or Obsolete_Flag = 'N' Then 0 Else 1 End As Bit) As Obsolete_Flag,Cast(Case When Flag_Inactive = '' Or Flag_Inactive = 'N' Then 0 Else 1 End As Bit) As Flag_Inactive From V_Inventory_All A Left Join V_Inventory_Mst2 B On A.Part = B.Part And A.Location = B.Location Left Join V_Inventory_Cost C On A.Part = C.Part And A.Location = C.Locn Left Join V_Inv_Extra_Text D On A.Part = D.Part where A.Location = '' Order By A.Part,A.Location;",True)
		'Populate the quantity fields
		F.Data.DataTable.CreateFromSQL("dtTemp","con","Select rtrim(A.Part) as Part,A.Qty_OnHand as Qty_OnHand,A.Qty_OnOrder_PO+A.Qty_OnOrder_WO As OnOrder,A.Qty_Required as Qty_Required,A.Qty_Usage_Mo_01+A.Qty_Usage_Mo_02+A.Qty_Usage_Mo_03+A.Qty_Usage_Mo_04+A.Qty_Usage_Mo_05+A.Qty_Usage_Mo_06+A.Qty_Usage_Mo_07+A.Qty_Usage_Mo_08+A.Qty_Usage_Mo_09+A.Qty_Usage_Mo_10+A.Qty_Usage_Mo_11+A.Qty_Usage_Mo_12 As Usage_Last_12_Mos,convert(A.Qty_OnHand*A.Amt_Price,SQL_Float) As Ext_Price,convert(A.Qty_OnHand*C.Cost,SQL_Float) As Ext_Cost From V_Inventory_All A Left Join V_Inventory_Cost C On A.Part = C.Part where (A.Qty_OnHand > 0 or A.Qty_OnOrder_PO+A.Qty_OnOrder_WO > 0 or A.Qty_Required > 0 or A.Qty_Usage_Mo_01+A.Qty_Usage_Mo_02+A.Qty_Usage_Mo_03+A.Qty_Usage_Mo_04+A.Qty_Usage_Mo_05+A.Qty_Usage_Mo_06+A.Qty_Usage_Mo_07+A.Qty_Usage_Mo_08+A.Qty_Usage_Mo_09+A.Qty_Usage_Mo_10+A.Qty_Usage_Mo_11+A.Qty_Usage_Mo_12 > 0 or convert((A.Qty_OnHand*A.Amt_Price),SQL_Float) > 0 or convert((A.Qty_OnHand*C.Cost),SQL_Float) > 0) and a.location = '' Order By A.Part;",True)
		'F.Data.Dictionary.CreateFromDataTable("dOnHand","dtTemp","Part","Qty_OnHand",4)
		v.Local.sSQL.Set("Select rtrim(Part) as Part, SUM(QTY_ONHAND) FROM V_INVENTORY_ALL WHERE LOCATION <> 'VC' GROUP BY PART")
		F.Data.Dictionary.CreateFromSQL("dOnHand","con",v.Local.sSQL)
		F.Data.Dictionary.SetDefaultReturn("dOnHand",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","dOnHand","Part","Qty_OnHand")
		F.Data.Dictionary.Close("dOnHand")
		F.Data.Dictionary.CreateFromDataTable("dOnOrder","dtTemp","Part","OnOrder",4)
		F.Data.Dictionary.SetDefaultReturn("dOnOrder",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","dOnOrder","Part","OnOrder")
		F.Data.Dictionary.Close("dOnOrder")
		F.Data.Dictionary.CreateFromDataTable("dQtyReqd","dtTemp","Part","Qty_Required",4)
		F.Data.Dictionary.SetDefaultReturn("dQtyReqd",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","dQtyReqd","Part","Qty_Required")
		F.Data.Dictionary.Close("dQtyReqd")
		F.Data.Dictionary.CreateFromDataTable("dUsage12mos","dtTemp","Part","Usage_Last_12_Mos",4)
		F.Data.Dictionary.SetDefaultReturn("dUsage12mos",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","dUsage12mos","Part","Usage_Last_12_Mos")
		F.Data.Dictionary.Close("dUsage12mos")
		F.Data.Dictionary.CreateFromDataTable("dExtPrice","dtTemp","Part","Ext_Price",4)
		F.Data.Dictionary.SetDefaultReturn("dExtPrice",0)
		F.Data.DataTable.FillFromDictionary("dtXStatus","dExtPrice","Part","Ext_Price")
		F.Data.Dictionary.Close("dExtPrice")
'		F.Data.Dictionary.CreateFromDataTable("dExtCost","dtTemp","Part","Ext_Cost",4)
'		F.Data.Dictionary.SetDefaultReturn("dExtCost",0)
'		F.Data.DataTable.FillFromDictionary("dtXStatus","dExtCost","Part","Ext_Cost")
'		F.Data.Dictionary.Close("dExtCost")
		F.Data.DataTable.DeleteRow("dtTemp")
		F.Data.DataTable.Close("dtTemp")
		f.Data.DataTable.AddExpressionColumn("dtXStatus","Ext_Cost","Float","Qty_Onhand*Cost")
		Gui.frmXStatus.pbStatus.Value(50)
	F.Intrinsic.Control.Else
		V.Local.dCutoffDate.Set(V.Screen.frmXStatus!dtpCutoff.Value)
		'Find Part+Loc with data after the Cutoff
		F.Intrinsic.String.Build("Select Distinct Part+'*!*'+Location From V_Inventory_Hist Where Date_History > '{0}' And Part <> '' And SubString(Part,17,1) <> '*' Order By Part,Location;",V.Local.dCutoffDate.PervasiveDate,V.Local.sSQL)
		F.ODBC.Connection!con.ExecuteReturn(V.Local.sSQL,V.Local.sPart)
		F.Intrinsic.String.Replace(V.Local.sPart,"'","''",V.Local.sPart)
		F.Intrinsic.String.Replace(V.Local.sPart,"#$#","','",V.Local.sPart)
		Gui.frmXStatus.pbStatus.Value(5)
		'Get information for the Part+Loc above
		F.Intrinsic.String.Build("Select Distinct A.Part+A.Location As PartLoc,A.Part,A.Location,RTrim(A.Description) As Description,RTrim(RTrim(B.Description_2)+' '+RTrim(B.Description_3)) as AltDesc,RTrim(D.Text) As ExtraDesc,A.Product_Line,Case B.Code_Source When 'P' Then 'PS' When 'J' Then 'PJ' When '' Then 'PJ' When 'M' Then 'MS' When 'F' Then 'MJ' Else B.Code_Source End As Code_Source,A.UM_Inventory,A.Time_Material_Lead,A.Code_Sort,A.Bin,A.Code_ABC,Cast(Null As Date) As Date_History,Cast(0 As Numeric(12,4)) As Qty_OnHand,Cast(0 As Numeric(12,4)) As Qty_Reorder,Cast(0 As Numeric(12,4)) As OnOrder,Cast(0 As Numeric(12,4)) As Qty_Required,Cast(0 As Numeric(12,4)) As Usage_Last_12_Mos,A.Amt_Price As Amt_Price,Cast(0 As Numeric(12,4)) As Cost,Cast(0 As Numeric(12,4)) As Matl,Cast(0 As Numeric(12,4)) As Labor,Cast(0 As Numeric(12,4)) As Ovhd,Cast(0 As Numeric(12,4)) As Outs,Cast(0 As Numeric(12,4)) As Freight,Cast(0 As Numeric(12,4)) As Oth,Cast(Case When Flag_Lot = '' Or Flag_Lot = 'N' Then 0 Else 1 End As Bit) As Flag_Lot,Cast(Case When Flag_Drop = '' Or Flag_Drop = 'N' Then 0 Else 1 End As Bit) As Flag_Drop,Cast(Case When Flag_Serialize = '' Or Flag_Serialize = 'N' Then 0 Else 1 End As Bit) As Flag_Serialize,Cast(Case When Obsolete_Flag = '' Or Obsolete_Flag = 'N' Then 0 Else 1 End As Bit) As Obsolete_Flag,Cast(Case When Flag_Inactive = '' Or Flag_Inactive = 'N' Then 0 Else 1 End As Bit) As Flag_Inactive From V_Inventory_Mstr A Left Join V_Inventory_Mst2 B On A.Part = B.Part And A.Location = B.Location Left Join V_Inventory_Hist C On A.Part = C.Part And A.Location = C.Location Left Join V_Inv_Extra_Text D On A.Part = D.Part Where C.Date_History <= '{0}' And A.Part+A.Location In ('{1}') Order By A.Part,A.Location;",V.Local.dCutoffDate.PervasiveDate,V.Local.sPart,V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtX1","con",V.Local.sSQL,True)
		Gui.frmXStatus.pbStatus.Value(15)
		'Get Inv History information for the Part+Loc above
		F.Intrinsic.String.Build("Select Part+Location As PartLoc,Part,Location,Date_History,Inv_Hist_Time,New_OnHand,New_Inv_Cost,New_Matl,New_Labr,New_Ovhd,New_Outs,New_Frgt,New_Oth From V_Inventory_Hist Where Date_History <= '{0}' And Part+Location In ('{1}') Order By Part,Location,Date_Action Desc,Inv_Hist_Time Desc;",V.Local.dCutoffDate.PervasiveDate,V.Local.sPart,V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtXdic","con",V.Local.sSQL,True)
		Gui.frmXStatus.pbStatus.Value(20)
		'Date_History
		F.Data.Dictionary.CreateFromDataTable("dDateHistory","dtXdic","PartLoc","Date_History")
		F.Data.Dictionary.SetDefaultReturn("dDateHistory",1/1/1900)
		F.Data.DataTable.FillFromDictionary("dtX1","dDateHistory","PartLoc","Date_History")
		F.Data.Dictionary.Close("dDateHistory")
		Gui.frmXStatus.pbStatus.Value(25)
		'Qty_OnHand
		F.Data.Dictionary.CreateFromDataTable("dNewOnHand","dtXdic","PartLoc","New_OnHand")
		F.Data.Dictionary.SetDefaultReturn("dNewOnHand",0.0)
		F.Data.DataTable.FillFromDictionary("dtX1","dNewOnHand","PartLoc","Qty_OnHand")
		F.Data.Dictionary.Close("dNewOnHand")
		Gui.frmXStatus.pbStatus.Value(30)
		'Cost
		F.Data.Dictionary.CreateFromDataTable("dNewInvCost","dtXdic","PartLoc","New_Inv_Cost")
