Program.Sub.LoaddtXStatus_Lot.Start
V.Local.iIndex.Declare
V.Local.sDefault.Declare
V.Local.sGV.Declare
V.Local.sProperty.Declare
V.Local.sSQL.Declare

V.Local.sSQL.Set("select rtrim(A.Part) as PartLoc, A.LOCATION, A.LOT, A.HEAT, A.BIN, CAST(IF(rtrim(a.serial_number) = '',1,a.serial_number) as Integer) as serial_number, A.QUANTITY, CAST(if (B.LENGTH = 0, 1, B.LENGTH) AS INTEGER) AS LENGTH, CAST(if (B.WIDTH = 0, 1,B.WIDTH) AS INTEGER) AS WIDTH, CAST(A.SERIAL_NUMBER AS INTEGER) AS SERIAL, A.ALLOCATED, A.USER_FIELD_5, A.ALLOC_TYPE from V_ITEM_MASTER A left outer join V_INVENTORY_ALL B on A.PART = B.PART and A.LOCATION = B.LOCATION where QUANTITY >0 and A.PART in (Select Part from V_INVENTORY_ALL where LOCATION = '');")

F.Intrinsic.Control.If(V.DataTable.dtXStatus$Lot.Exists,=,True)
	F.Data.DataTable.DeleteRow("dtXStatus$Lot")
	F.Data.DataTable.Close("dtXStatus$Lot")
F.Intrinsic.Control.EndIf

F.Data.DataTable.CreateFromSQL("dtXStatus$Lot","con",V.Local.sSQL,True)

F.Data.DataTable.AddRelation("dtXStatus","Part","dtXStatus$Lot","PartLoc")

'/*case when CAST(A.SERIAL_NUMBER as FLOAT) > 0 then round(A.QUANTITY/CAST(A.SERIAL_NUMBER as FLOAT),0) else A.QUANTITY end as PIECES,
'(case when B.LENGTH = 0 THEN A.QUANTITY WHEN cast(A.SERIAL_NUMBER as FLOAT) = B.LENGTH then ROUND(A.QUANTITY/(B.LENGTH*b.width/144),0) ELSE 0 END) as FullPcs, 
'(CASE WHEN B.LENGTH > 0 AND cast(A.SERIAL_NUMBER as FLOAT) <> B.LENGTH THEN A.QUANTITY ELSE 0 END) AS DROPS, */

'DETERMINES IS THE QTY SHOULD BE USED FOR DROPS OR NOT.  IF SERIAL DOES NOT EQUAL INV MSTR LENGTH THEN THIS ITEM MASTER RECORD IS CONSIDERED A DROP, IF IT IS FULL LENGTH THEN SET AS 0
f.Data.DataTable.AddExpressionColumn("dtXStatus$Lot","DROPS",Float,"IIF(SERIAL <> LENGTH,QUANTITY,0)")
'IF IT IS A DROP ITEM MASTER RECORD, IT WILL EVALUATE HOW MANY PIECES, IF NOT, ,IT WILL JUST END UP 0
f.Data.DataTable.AddExpressionColumn("dtXStatus$Lot","Drop_pieces",Float,"IIF(SERIAL*WIDTH <> 0,convert((DROPS/(SERIAL*WIDTH/144)),'System.Int32'),0)")
'IF IT IS A FULL PIECE (SERIAL = INV_MSTR.LENGTH) THEN IT WILL CALCULATE THE NUMBER OF PIECES, OTHERWISE 0
f.Data.DataTable.AddExpressionColumn("dtXStatus$Lot","FullPcs",Float,"IIF(CONVERT(SERIAL,'System.Int32') = LENGTH,(convert((QUANTITY/(LENGTH*WIDTH/144)),'System.Int32')),0)")

F.Data.DataView.Create("dtXStatus$Lot","dvLot")

V.Local.sGV.Set("gvLot")

gui.frmXStatus.GsGCXStatus.AddGridviewFromDataview(V.Local.sGV,"dtXStatus","dvLot")

gui.frmXStatus.GsGCXStatus.SetGridviewProperty(V.Local.sGv,"AllowAddRows",False)
gui.frmXStatus.GsGCXStatus.SetGridviewProperty(V.Local.sGv,"AllowDeleteRows",False)
gui.frmXStatus.GsGCXStatus.SetGridviewProperty(V.Local.sGv,"Editable",False)
gui.frmXStatus.GsGCXStatus.SetGridviewProperty(V.Local.sGv,"MultiSelect",False)
gui.frmXStatus.GsGCXStatus.SetGridviewProperty(V.Local.sGv,"ShowGroupPanel",False)

